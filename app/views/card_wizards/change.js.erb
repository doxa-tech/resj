goToByScroll($('.head'));
$('.wizard').html('<%=j render "#{@card.current_step}_step" %>');
<% case @card.current_step %>
	<% when "general" %>
		selectize.cards();
		selectize.default();
	<% when "location" %>
		selectize.location();
		load_google_map.loadScript('card_new_map', null, true);
	<% when "team" %>
		autocomplete();
		hide_contact()
	<% when "extra" %>
		selectize.tags();
<% end %>