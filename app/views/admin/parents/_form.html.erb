<%= form_for [:admin, @parent] do |f| %>

	<%= render 'error_messages', object: f.object %>

	<%= f.label :user_id %>
	<%= f.collection_select :user_id, User.all, :id, :full_name %>

	<%= f.label :parent_id %>
	<%= f.collection_select :parent_id, User.all, :id, :full_name %>

	<%= f.submit %>

<% end %>